  "description": "PHP BE 双驱框架（Beyond Exception），支持普通PHP和Swoole两种方式部署，兼顾便捷与性能。",
  "keywords": [
    "PHP",
    "Be",
    "双驱框架",
    "Beyond Exception"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "1024i team",
      "email": "1024i@1024i.com",
      "homepage": "https://www.1024i.com/team"
    }
  ],
  "require": {
    "php": "~7.4",
    "be/ui": "*",
    "aliyuncs/oss-sdk-php": "~2.4",
    "qcloud/cos-sdk-v5": "2.*",
    "elasticsearch/elasticsearch": "^v7.11",
    "phpmailer/phpmailer": "~6.0",
    "phpoffice/phpspreadsheet": "^1.8",
    "ext-bcmath": "*",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-pdo": "*",
    "ext-gd": "*",
    "ext-openssl": "*"
  },
  "autoload": {
    "psr-4": {
      "Be\\": "src"
    },
    "files": [
      "src/functions.php"
    ]
  }
}
